{
  "name": "appseed-firebase",
  "version": "1.0.0",
  "description": "Simple starting kit for working with firebase along with some CLI to add new react components/containers",
  "main": "index.js",
  "scripts": {
    "start": "parcel www/index.html -d .temp --port 1234",
    "build": "node ./tools/build-production",
    "postbuild": "node ./tools/build-copy-data",
    "firebase:deploy": "firebase deploy",
    "deploy": "npm-run-all build firebase:deploy",
    "new": "node ./tools/cli new"
  },
  "author": "Phil Willis <pdotwdot@gmail.com> (http://www.philwillis.pw/)",
  "license": "ISC",
  "dependencies": {
    "react": "16.4.2",
    "react-dom": "16.3.2",
    "styled-components": "3.3.3"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chalk": "2.4.1",
    "copy-dir": "0.3.0",
    "fs-extra": "7.0.0",
    "inquirer": "6.2.0",
    "lodash": "4.17.10",
    "node-sass": "4.9.0",
    "npm-run-all": "4.1.3",
    "parcel-bundler": "1.8.1"
  }
}
